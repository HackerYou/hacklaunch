<table class='table'>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% @sign_ups.each do |sign_up| %>
      <tr>
        <td><%= sign_up.name %></td>
        <td><%= sign_up.email %></td>
        <td><%= link_to "Edit", edit_sign_up_path(sign_up) %></td>
      </tr>
    <% end %>
  </tbody>
</table>